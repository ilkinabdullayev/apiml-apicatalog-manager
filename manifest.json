 {
    "name": "API Mediation Layer - API Catalog manager",
    "version": "2.0",
    "description": "It allows to check job logs, stop the jobs, check eureka metadata, modify static api defs and refresh eureka with them without restarting it.!",
    "permissions": [
      "tabs", 
      "activeTab",
       "declarativeContent", 
       "storage",
       "http://*/*",
       "https://*/*",
       "webRequest",
       "webNavigation",
       "*://*.google.com/"
      ],
      "web_accessible_resources": ["fullscreenbutton.css","statics/glyphicon.css", "fonts/*", "banner/append_banner.html"],
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "48": "images/logo-48.png",
        "128": "images/logo-128.png"
      }
    },
   "options_page": "options/options.html",
    "icons": {
      "16": "images/logo-16.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  }

