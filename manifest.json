 {
    "name": "API Mediation Layer - API Catalog manager",
    "version": "1.0",
    "description": "You could start and stop dynamic services!",
    "permissions": [
      "tabs", 
      "activeTab",
       "declarativeContent", 
       "storage",
       "http://*/*",
       "https://*/*",
       "webRequest",
       "webNavigation",
       "*://*.google.com/"
      ],
      "web_accessible_resources": ["banner/append_banner.html"],
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "48": "images/logo-48.png",
        "128": "images/logo-128.png"
      }
    },
   "options_page": "options/options.html",
    "content_scripts": [
      {
        "matches": ["https://localhost:10010/ui/v1/apicatalog/*"],
        "js": ["contentScript.js"]
      }
    ],
    "content_security_policy": "script-src 'self' https://localhost:10010/ui/v1/apicatalog; object-src 'self'",
    "icons": {
      "16": "images/logo-16.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  }

